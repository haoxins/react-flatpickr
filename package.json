{
  "name": "react-flatpickr",
  "version": "4.0.0",
  "description": "flatpickr for React",
  "main": "build/react-flatpickr",
  "module": "build/react-flatpickr.mjs",
  "exports": {
    ".": {
      "import": "./build/react-flatpickr.mjs",
      "require": "./build/react-flatpickr.js"
    }
  },
  "scripts": {
    "example": "npm run build && webpack --config example/webpack.js",
    "dev": "echo 'Please open the example directory to view the example' && webpack-dev-server --config example/webpack.js",
    "lint": "eslint lib && eslint example",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": "coderhaoxin/react-flatpickr",
  "keywords": [
    "flatpickr",
    "react"
  ],
  "files": [
    "build"
  ],
  "author": "haoxin",
  "license": "MIT",
  "dependencies": {
    "flatpickr": "^4.6.2",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-eslint": "^9.0.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^7.20.0",
    "jest": "^24.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "semantic-ui-react": "^0.88.2",
    "vite": "^4.2.1"
  },
  "peerDependencies": {
    "react": ">=16, <=18"
  },
  "eslintConfig": {
    "extends": [
      "ok"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ]
  }
}
