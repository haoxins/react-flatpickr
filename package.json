{
  "name": "react-flatpickr",
  "version": "3.10.13",
  "description": "flatpickr for React",
  "main": "build/index.js",
  "scripts": {
    "example": "npm run build && webpack --config example/webpack.js",
    "dev": "echo 'Please open the example directory to view the example' && webpack-dev-server --config example/webpack.js",
    "lint": "eslint lib && eslint example",
    "build": "babel lib --out-dir build",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": "coderhaoxin/react-flatpickr",
  "keywords": [
    "flatpickr",
    "react"
  ],
  "files": [
    "build/index.js",
    "lib/"
  ],
  "author": "haoxin",
  "license": "MIT",
  "dependencies": {
    "flatpickr": "^4.6.2",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^7.20.0",
    "eslint-config-ok": "github:haoxins/eslint-config",
    "jest": "^24.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "semantic-ui-react": "^0.88.2",
    "style-loader": "^0.19.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "react": ">=16, <=18"
  },
  "eslintConfig": {
    "extends": [
      "ok"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ]
  }
}
